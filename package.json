{
  "name": "django-browser-preload",
  "private": true,
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "curly": ["error", "all"],
      "dot-notation": ["error", {"allowKeywords": true}],
      "eqeqeq": ["error"],
      "no-alert": ["off"],
      "no-eval": ["error"],
      "no-script-url": ["error"],
      "no-shadow": ["error"],
      "no-var": ["error"],
      "prefer-const": ["error"],
      "semi": ["error", "always"],
      "strict": ["error", "global"]
    },
    "env": {
      "browser": true,
      "es6": true,
      "worker": true
    }
  }
}
